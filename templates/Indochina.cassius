#background
    background: url("../static/img/montanha.jpg")
    background-repeat: no-repeat;
    background-size:cover;
    background-position: center;
    background-attachment: fixed;
   

#logo img
  margin-top: 50px
  margin-bottom: 50px
  margin-left: 40px
  width: 200px


#menu
  overflow: hidden;
  background-color: #000;
  
    

#menu a 
  float: left
  color: #FFF
  text-align: center
  padding: 14px 16px
  text-decoration: none
  font-size: 17px


#menu a:hover 
  background-color: #00C2FF
  color: black


#menu a:active 
  background-color: #00C2FF
  color: white


