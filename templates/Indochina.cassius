#link-content
    text-decoration: none 
    font-size: 1.5em
    max-width:400px
    margin:150px auto
    text-align:center

#link-content a:link
    color: white
    background-color: transparent
    text-decoration: none

#link-content a:visited
    color: blue
    background-color: transparent
    text-decoration: none

#link-content a:hover
    color: yellow;
    background-color: transparent
    text-decoration: underline

#link-content a:active
    color: yellow
    background-color: transparent
    text-decoration: underline   

